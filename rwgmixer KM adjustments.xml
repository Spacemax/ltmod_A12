<?xml version="1.0" encoding="utf-8"?>
<rwgmixer>
	<world ruleset="vanilla" />

	<rulesets>
		<ruleset name="vanilla" cell_cache_size="32" cell_size="1200" cell_offset="0.4" generation_distance="10000">
			<cell_rule name="zeroZero" position="0,0" prob="0.1"/>
			<cell_rule name="wastelandHub" prob="0.05"/>
			<cell_rule name="highDensity" prob="0.45"/>
			<cell_rule name="lowDensity" prob="0.5"/>
			<!-- <cell_rule name="wastelandHub" prob="0.05"/> -->
			<!--<cell_rule name="customHubTest" position="0,1" prob="0"/>-->
		</ruleset>
	</rulesets>

	<cell_rules>
		
		<cell_rule name="lowDensity">
			<cave_count value="3,7"/>
			<path_material value="asphalt" />
			<path_radius value="6" />
			<!-- <hub_rule name="citySmall" prob="0.1"/> -->
			<hub_rule name="townSmall" prob="0.2"/>
			<hub_rule name="ruralSmall" prob="0.7"/>
			<wilderness_rule name="wildernessDefault"/>
		</cell_rule>

		<cell_rule name="highDensity">
			<cave_count value="1,3"/>
			<path_material value="asphalt" />
			<path_radius value="7" />
			<hub_rule name="townSmall" prob="0.35"/>
			<hub_rule name="townLarge" prob="0.65"/>
			<wilderness_rule name="wildernessDefault" prob="1"/>
		</cell_rule>

		<cell_rule name="wastelandHub">
			<cave_count value="0,0"/>
			<path_material value="asphalt" />
			<path_radius value="8" />
			<hub_rule name="citySmall" />
			<wilderness_rule name="wildernessDefault" prob="1"/>
		</cell_rule>

		<cell_rule name="zeroZero">
			<cave_count value="0,0"/>
			<path_material value="asphalt" />
			<path_radius value="8" />
			<hub_rule name="zeroCity" />
			<wilderness_rule name="wildernessDefault" prob="1"/>
		</cell_rule>
	
		<!-- Rules Not in use at this time -->
		
		<!-- <cell_rule name="customHubTest">
			<cave_count value="1,4"/>
			<path_material value="asphalt" />
			<path_radius value="7" />
			<hub_rule name="customHubTest"/>
			<wilderness_rule name="wildernessDefault"/>
		</cell_rule> -->
	
	</cell_rules>

	<hub_rules>
		<hub_rule name="ruralSmall">
			<hub_type value="rural"/>
			<width value="96, 96" />
			<height value="96, 96" />
			<path_material value="gravel" />
			<path_radius value="5" />
			<prefab_rule name="ruralSmall" prob="1"/>
		</hub_rule>

		<hub_rule name="townSmall">
			<hub_type value="town"/>
			<width value="120, 160" />
			<height value="120, 160" />
			<path_material value="asphalt" />
			<path_radius value="5" />
			<prefab_rule name="townSmall" prob="1"/>
		</hub_rule>
		
		<hub_rule name="townLarge">
			<hub_type value="town"/>
			<width value="160, 240" />
			<height value="160, 240" />
			<path_material value="asphalt" />
			<path_radius value="6" />
			<prefab_rule name="townLarge"/>
		</hub_rule>

		<hub_rule name="citySmall">
			<hub_type value="city"/>
			<width value="206, 414" />
			<height value="206, 414" />
			<path_material value="asphalt" />
			<path_radius value="7" />
			<prefab_rule name="wastelandHub" prob="1"/>
		</hub_rule>

		<hub_rule name="zeroCity">
			<hub_type value="city"/>
			<width value="240,240" />		<!-- Was 480 -->
			<height value="240,240" />
			<path_material value="asphalt" />
			<path_radius value="8" />
			<prefab_rule name="wastelandHub" prob="1"/>
		</hub_rule>
		
